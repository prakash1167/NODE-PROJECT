<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>Project - Checkout</title>
	<link rel="stylesheet" href="/css/style.css">
</head>
<body>
	<div class="main-content">
		<header class="header bg-white" id="header">
			<div class="container-fluid">
				<div class="page-header">
					<div class="header-left">
						<div class="logo-image">
				            <a href="/admin">
				            	<img src="/images/wigo-logo-img.png" width="70" height="36" alt="Wigo Store" itemprop="logo">
				            </a>
				      	</div>
			      	</div>
			      	<div class="header-right">
			      		<div class="header-items">
				          	<span class="sign-in">
			            		<svg width="22px" height="22px" viewBox="-1 -1 21 20">
								  	<g transform="translate(0.968750, -0.031250)" stroke="none" stroke-width="1" fill="currentColor" fill-rule="nonzero">
								    	<path d="M9,7.5 C10.704,7.5 12.086,6.157 12.086,4.5 C12.086,2.843 10.704,1.5 9,1.5 C7.296,1.5 5.914,2.843 5.914,4.5 C5.914,6.157 7.296,7.5 9,7.5 Z M9,9 C6.444,9 4.371,6.985 4.371,4.5 C4.371,2.015 6.444,0 9,0 C11.556,0 13.629,2.015 13.629,4.5 C13.629,6.985 11.556,9 9,9 Z M1.543,18 L0,18 L0,15 C0,12.377 2.187,10.25 4.886,10.25 L14.143,10.25 C16.273,10.25 18,11.929 18,14 L18,18 L16.457,18 L16.457,14 C16.457,12.757 15.421,11.75 14.143,11.75 L4.886,11.75 C3.04,11.75 1.543,13.205 1.543,15 L1.543,18 Z"></path>
								  	</g>
								</svg>
								<div class="head-link bg-white">
									<a class="main-menu-link" href="/admin/logout">Logout</a>
								</div>
				          	</span>
				        </div>
					</div>
				</div>
			</div>
		</header>

		<section class="flex">
			<div class="container-fluid">
				<div class="admin-content">
					<div class="admin-left-nav mt-20">
						<ul>
							<li><a href="/admin/products">Products</a></li>
							<li><a class="active" href="javascript:;">Orders</a></li>
							<li><a href="/admin/discounts">Discounts</a></li>
						</ul>
					</div>
					<div class="admin-right page-content">
						<div class="products-list">
							<div class="actions flex items-center">
								<h3>Orders</h3>
								<!-- <a href="add-product.html" class="button button--hollow justify-end inline-block">Create Order</a> -->
							</div>
							<div class="actions flex items-center flex-start">
								<span><span id="count">1</span> selected</span>
								<a href="/admin/editorder/" class="white-btn items-center">Edit Orders</a>
							</div>
							<div class="added-products">
								<table class="table">
									<thead>
										<tr>
											<th><input type="checkbox" id="select-all"></th>
											<th>Order No.</th>
											<th>Date</th>
											<th>Payment Status</th>
											<th>Fulfillment Status</th>
											<th>Items</th>
											<th class="text-right">Total</th>
										</tr>
									</thead>
									<tbody><%if (Orders.length>0){%>
										<%for(let Order of Orders){%>
										<tr>
											<td><input type="checkbox" name="prod-item"></td>
											<td><a href="/admin/editorder/<%=Order.id%>"><u><%=Order.order_no%></u></a></td>
											<td><%=Order.date%></td>
											<td><%=Order.payment_status%></td>
											<td><%=Order.fulfillment_status%></td>
											<td><%=Order.cart.length%></td>
											<td class="text-right">$<%=Order.total_price%></td>
										</tr><%}%>
										<% } else { %>
										 <tr>
										<td>No data found</td>
									</tr><%}%>
										
									</tbody>
								</table>
							</div>
						</div>
					</div>
				</div>
			</div>
		</section>
		
		<footer class="footer bg-white">
			<div class="container-fluid">
				This is footer section
			</div>
		</footer>
	</div>
</body>
<script>
	document.getElementById('select-all').onclick = function() {
	    var checkboxes = document.querySelectorAll('input[type="checkbox"]');
	    for (var checkbox of checkboxes) {
	        checkbox.checked = this.checked;
	    }
	}
</script>
</html>